{
  "navigationFallback": {
    "rewrite": "/login.html"
  },
  "routes": [
    {
      "route": "/login.html",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login.html"
    }
  }
}